# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

GET 	/manage						controllers.Manage.index()

GET		/debug						controllers.Debug.index()
GET		/debug/save/:name			controllers.Debug.saveCarrier(name : String)
GET		/debug/delete/:id			controllers.Debug.deleteCarrier(id : Long)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

# GET	/routetest/:id				controllers.Blah.blah(id : Long)